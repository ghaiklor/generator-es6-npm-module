{
  "name": "generator-es6-npm-module",
  "description": "Yeoman generator for starting ES6 npm module with Mocha, Istanbul, Travis, Coveralls",
  "license": "MIT",
  "main": "generators/app/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ghaiklor/generator-es6-npm-module.git"
  },
  "author": {
    "name": "Eugene Obrezkov",
    "email": "ghaiklor@gmail.com",
    "url": "https://github.com/ghaiklor"
  },
  "scripts": {
    "clean": "rm -rf ./generators",
    "compile": "npm run clean; cp -r src/ generators/; babel src --out-dir generators",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "prepublish": "npm run compile",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "babel-node ./node_modules/.bin/isparta cover _mocha"
  },
  "keywords": [
    "yeoman-generator",
    "boilerplate",
    "scaffolder",
    "npm",
    "module",
    "ES6"
  ],
  "dependencies": {
    "yeoman-generator": "0.21.1",
    "yosay": "1.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.3.17",
    "babel-plugin-add-module-exports": "0.1.1",
    "babel-preset-es2015-node4": "2.0.2",
    "coveralls": "2.11.5",
    "cz-conventional-changelog": "1.1.5",
    "isparta": "4.0.0",
    "mocha": "2.3.4",
    "semantic-release": "4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "tag": "latest"
  },
  "release": {
    "branch": "master"
  }
}

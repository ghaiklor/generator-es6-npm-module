{
  "name": "generator-es6-npm-module",
  "version": "0.0.0-semantic-release",
  "description": "Yeoman generator for starting ES6 npm module with Mocha, Istanbul, Travis, Coveralls",
  "license": "MIT",
  "main": "generators/app/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ghaiklor/generator-es6-npm-module.git"
  },
  "author": {
    "name": "Eugene Obrezkov",
    "email": "ghaiklor@gmail.com",
    "url": "https://github.com/ghaiklor"
  },
  "scripts": {
    "coveralls": "cat coverage/lcov.info | coveralls",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "istanbul cover _mocha"
  },
  "keywords": [
    "yeoman-generator",
    "boilerplate",
    "scaffolder",
    "npm",
    "module",
    "ES6"
  ],
  "dependencies": {
    "yeoman-generator": "0.22.5",
    "yosay": "1.1.0"
  },
  "devDependencies": {
    "coveralls": "2.11.9",
    "cz-conventional-changelog": "1.1.5",
    "istanbul": "0.4.2",
    "mocha": "2.4.5",
    "semantic-release": "4.3.5",
    "yeoman-assert": "2.1.2",
    "yeoman-test": "1.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "tag": "latest"
  },
  "release": {
    "branch": "master"
  }
}

  "keywords": [
    "yeoman-generator",
    "boilerplate",
    "scaffolder",
    "npm",
    "module",
    "ES6"
  ],
  "dependencies": {
    "yeoman-generator": "0.21.1",
    "yosay": "1.0.5"
  },
  "devDependencies": {
    "babel": "6.0.0",
    "istanbul": "0.4.0",
    "mocha": "2.3.3"
  }
